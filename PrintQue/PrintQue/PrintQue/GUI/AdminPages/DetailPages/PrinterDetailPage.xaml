<?xml version="1.0" encoding="utf-8" ?>
<ContentPage 

    Title="New Printer"
    xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PrintQue.GUI.AdminPages.RequestDetailPage">
    <ContentPage.ToolbarItems>
        <ToolbarItem Icon="save.png" Activated="ToolbarItem_Save_Activated" Order="Primary"/>
    </ContentPage.ToolbarItems>
    <TableView Intent="Form">
        <TableRoot>
            <TableSection Title="Printer Details">
                <EntryCell x:Name="ent_Name" Label="Printer Name" Placeholder="Type Here..."/>
                <EntryCell x:Name="ent_Status" Label="Printer Status" Placeholder="Type Here..."/>
                <EntryCell x:Name="ent_Color" Label="Printer Color" Placeholder="Type Here..."/>
            </TableSection>
        </TableRoot>
    </TableView>


</ContentPage>