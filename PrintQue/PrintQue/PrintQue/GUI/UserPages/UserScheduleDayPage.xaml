<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PrintQue.GUI.UserPages.UserScheduleDayPage">
    <ContentPage.Content>

        <StackLayout>
            <StackLayout x:Name="CalendarStackLayout"></StackLayout>
            <Label HorizontalTextAlignment="Center" x:Name="SelectedDateLabel"></Label>
            <Button Text="Submit Date" Clicked="SubmitDate_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>