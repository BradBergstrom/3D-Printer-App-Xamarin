<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    Title="New Status"
            xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PrintQue.GUI.DetailPages.StatusDetailPage">
    <ContentPage.ToolbarItems>
        <ToolbarItem Icon="save.png" Activated="ToolbarItem_Save_Activated" Order="Primary"/>
    </ContentPage.ToolbarItems>
    <TableView Intent="Form">
        <TableRoot>
            <TableSection Title="Status Details">
                <EntryCell x:Name="ent_Name" Label="Status Name" Placeholder="Type Here..."/>
                
            </TableSection>
        </TableRoot>
    </TableView>
</ContentPage>